{
  "name": "status-board",
  "productName": "Status Board",
  "description": "App description",
  "author": {
    "name": "Sean Watkins",
    "email": "seanlw@gmail.com"
  },
  "homepage": "https://lab.spidy.org/sean/status-board",
  "version": "3.0.0-alpha.1",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pack": "yarn compile && electron-builder --dir"
  },
  "build": {
    "productName": "Status Board",
    "appId": "com.example",
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "classnames": "^2.2.6",
    "electron-window-state": "^5.0.3",
    "event-kit": "^2.5.3",
    "file-url": "^2.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-transition-group": "^1.2.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "source-map-support": "^0.5.9",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/event-kit": "^2.4.0",
    "@types/file-url": "^2.0.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-transition-group": "1.1.1",
    "@types/request-promise": "^4.1.43",
    "@types/uuid": "^3.4.4",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "electron-webpack": "^2.6.1",
    "electron-webpack-ts": "^3.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.26.0"
  },
  "resolutions": {
    "webpack-sources": "1.3.0"
  }
}
